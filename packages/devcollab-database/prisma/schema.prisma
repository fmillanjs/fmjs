generator client {
  provider = "prisma-client-js"
  output   = "../../../node_modules/.prisma/devcollab-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DEVCOLLAB_DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String?
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([email])
}
