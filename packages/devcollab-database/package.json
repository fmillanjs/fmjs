    "bcrypt": "^6.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "prisma": "^5.22.0",
    "tsx": "^4.21.0",
    "typescript": "~5.6.0"
  }
}
